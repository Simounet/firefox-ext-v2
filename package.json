{
  "title": "Wallabag",
  "name": "wallabag-firefox",
  "version": "0.0.1",
  "description": "Wallabag is an open-source and self-hosted alternative to Pocket, Instapaper, etc.",
  "main": "index.js",
  "author": "Thibaud Dauce",
  "engines": {
    "firefox": ">=38.0a1",
    "fennec": ">=38.0a1"
  },
  "license": "MIT",
  "keywords": [
    "jetpack"
  ],
  "preferences": [
    {
        "name": "wallabagUrl",
        "title": "Wallabag URL",
        "type": "string",
        "value": ""
    },
    {
        "name": "wallabagClientId",
        "title": "Client ID.",
        "type": "string",
        "value": ""
    },
    {
        "name": "wallabagSecretId",
        "title": "Secret ID.",
        "type": "string",
        "value": ""
    },
    {
        "name": "wallabagToken",
        "title": "Connection token",
        "type": "string",
        "value": "",
        "hidden": true
    }
  ]
}
